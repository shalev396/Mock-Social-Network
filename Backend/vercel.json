{
  "version": 2,
  "builds": [
    {
      "src": "Backend/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/users/(.*)",
      "dest": "Backend/app.js"
    },
    {
      "src": "/api/posts/(.*)",
      "dest": "Backend/app.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "Backend/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "Backend/app.js"
    }
  ]
}
